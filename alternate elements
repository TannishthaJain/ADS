import java.util.*;

public class Main {
    public static void main(String[] args) {
        String[] arr = {"a", "b", "c", "d", "e", "f"};
        List<String> result = new ArrayList<>();

        for (int i = 0; i < arr.length; i += 2) {
            result.add(arr[i]);
        }

        for (int i = arr.length - 1; i >= 0; i -= 2) {
            result.add(arr[i]);
        }

        System.out.println(result);
    }
}
